<script>
        $(document).ready(function() {
    var $modalElement = $('#quoteModal');

    $modalElement.on("shown.bs.modal", function() {
        console.log("Modal opened:", $modalElement.attr('id'));
        captchaShowForQuote(); // Updated function
    });

    $modalElement.on("hidden.bs.modal", function() {
        console.log("Modal closed:", $modalElement.attr('id'));
        captchaRemoveForQuote(); // Updated function
    });

    function captchaShowForQuote() {
        // $("#quoteCap").show();
        $("#contactCaptchaDiv").empty();
        $('#quoteCaptchaDiv').html(`
            <div class="form-label-group col-md-12">
                <div class="input-group" id="quoteCaptchaWrapper">
                    <input type="text" class="form-control" placeholder="Captcha" id="quoteCaptcha"
                        name="quoteCaptcha" autocomplete="off" style="width: 30%" value="{{ old('quoteCaptcha') }}">
                    <label for="quoteCaptcha">Captcha</label>
                    <div class="input-group-append">
                        <span class="input-group-text p-0 border-0" style="background-color: #ffffff !important;">
                            @captcha
                        </span>
                    </div>
                </div>
            </div>
        `);
    }

    // function captchaRemoveForQuote() {
    //     $('#quoteCaptchaDiv').empty();
    // }
});
</script>

